55;ovsdb-cluster.at:45;garbage collection - cluster of 1;ovsdb server positive unix cluster cluster1;
56;ovsdb-cluster.at:54;insert default row, query table - cluster of 3;ovsdb server positive unix cluster cluster3;
57;ovsdb-cluster.at:54;insert row, query table - cluster of 3;ovsdb server positive unix cluster cluster3;
58;ovsdb-cluster.at:54;insert rows, query by value - cluster of 3;ovsdb server positive unix cluster cluster3;
59;ovsdb-cluster.at:54;insert rows, query by named-uuid - cluster of 3;ovsdb server positive unix cluster cluster3;
60;ovsdb-cluster.at:54;insert rows, update rows by value - cluster of 3;ovsdb server positive unix cluster cluster3;
